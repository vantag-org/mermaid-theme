#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/mermaid-theme/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_rollup@4.57.1_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_rollup@4.57.1_typescript@5.9.3_yaml@2.8.2/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/mermaid-theme/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_rollup@4.57.1_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_rollup@4.57.1_typescript@5.9.3_yaml@2.8.2/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.17.1_jiti@2.6.1_rollup@4.57.1_typescript@5.9.3_yaml@2.8.2/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.17.1_jiti@2.6.1_rollup@4.57.1_typescript@5.9.3_yaml@2.8.2/node_modules/astro/astro.js" "$@"
fi
