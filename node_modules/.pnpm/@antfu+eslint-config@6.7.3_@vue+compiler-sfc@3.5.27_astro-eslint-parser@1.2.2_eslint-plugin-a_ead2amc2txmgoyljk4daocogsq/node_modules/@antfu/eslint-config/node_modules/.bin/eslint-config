#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules/@antfu/eslint-config/bin/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules/@antfu/eslint-config/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules/@antfu/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules/@antfu/eslint-config/bin/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules/@antfu/eslint-config/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules/@antfu/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.27_astro-eslint-parser@1.2.2_eslint-plugin-a_ead2amc2txmgoyljk4daocogsq/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.mjs" "$@"
else
  exec node  "$basedir/../../bin/index.mjs" "$@"
fi
