#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/mermaid-theme/node_modules/.pnpm/bumpp@10.4.1_magicast@0.5.2/node_modules/bumpp/bin/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/bumpp@10.4.1_magicast@0.5.2/node_modules/bumpp/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/bumpp@10.4.1_magicast@0.5.2/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/mermaid-theme/node_modules/.pnpm/bumpp@10.4.1_magicast@0.5.2/node_modules/bumpp/bin/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/bumpp@10.4.1_magicast@0.5.2/node_modules/bumpp/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/bumpp@10.4.1_magicast@0.5.2/node_modules:/workspaces/mermaid-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bumpp/bin/bumpp.mjs" "$@"
else
  exec node  "$basedir/../bumpp/bin/bumpp.mjs" "$@"
fi
